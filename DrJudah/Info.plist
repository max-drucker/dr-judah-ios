	<string>Dr. Judah needs access to your health data to provide personalized insights, track trends, and alert you to important changes.</string>
	<key>NSHealthUpdateUsageDescription</key>
	<string>Dr. Judah may write health data back to Apple Health.</string>
	<key>UIBackgroundModes</key>
	<array>
		<string>fetch</string>
		<string>processing</string>
	</array>
	<key>BGTaskSchedulerPermittedIdentifiers</key>
	<array>
		<string>com.drjudah.healthsync</string>
		<string>com.drjudah.healthcheck</string>
	</array>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>drjudah</string>
			</array>
		</dict>
	</array>
</dict>
</plist>
